{
  "name": "arakne/swf",
  "description": "SWF parsing and extraction library",
  "keywords": ["swf", "svg", "flash"],
  "license": "LGPL-3.0-or-later",
  "type": "project",
  "authors": [
    {
      "name": "Vincent QUATREVIEUX",
      "email": "quatrevieux.vincent@gmail.com"
    }
  ],
  "require": {
    "php-64bit": "~8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~10.5",
    "vimeo/psalm": "~6.0",
    "friendsofphp/php-cs-fixer": "^3.62",
    "sapientpro/image-comparator": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Arakne\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Arakne\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "coverage": "XDEBUG_MODE=coverage phpunit --coverage-html=coverage"
  }
}
